	"license": "MIT",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test": "jest"
	},
	"dependencies": {
		"core-js": "latest",
		"css-element-queries": "latest",
		"mitt": "latest",
		"register-service-worker": "latest",
		"vue": "latest",
		"vue-fullscreen": "^3.1.1",
		"vue-router": "latest",
		"vue-yandex-maps": "^0.12.3",
		"vuex": "latest"
	},
	"devDependencies": {
		"@types/jest": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"@vue-leaflet/vue-leaflet": "latest",
		"@vue/cli-plugin-babel": "latest",
		"@vue/cli-plugin-eslint": "latest",
		"@vue/cli-plugin-router": "latest",
		"@vue/cli-plugin-typescript": "latest",
		"@vue/cli-plugin-vuex": "latest",
		"@vue/cli-service": "latest",
		"@vue/compiler-sfc": "latest",
		"@vue/eslint-config-standard": "latest",
		"@vue/eslint-config-typescript": "latest",
		"@vue/test-utils": "latest",
		"chromedriver": "latest",
		"eslint": "latest",
		"eslint-plugin-import": "latest",
		"eslint-plugin-node": "latest",
		"eslint-plugin-promise": "latest",
		"eslint-plugin-vue": "latest",
		"geckodriver": "latest",
		"jest": "latest",
		"lint-staged": "latest",
		"node-sass": "latest",
		"sass-loader": "latest",
		"ts-jest": "latest",
		"ts-node": "latest",
		"typescript": "latest",
		"vue3-jest": "latest",
		"wdio-chromedriver-service": "latest",
		"wdio-geckodriver-service": "latest"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,vue,ts,tsx}": [
			"vue-cli-service lint",
			"git add"
		]
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		}
	}
}
